<p id="notice"><%= notice %></p>

<h1>Access Groups</h1>
<div class="container-fluid" style="margin-top: 0px">

<table class= "table table-bordered">
  <thead class="table table-dark">
    <tr>
      <th>id</th>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @access_groups.each do |access_group| %>
      <tr>
        <td><%= access_group.id %></td>
        <td><%= access_group.name %></td>
        <td><%= link_to 'Show', access_group %></td>
        <td><%= link_to 'Edit', edit_access_group_path(access_group) %></td>
        <td><%= link_to 'Destroy', access_group, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

</div>

<%= link_to 'New Access Group', new_access_group_path %>

<p id="notice"><%= notice %></p>

<h1>Storage Places</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @storage_places.each do |storage_place| %>
      <tr>
        <td><%= storage_place.title%></td>
        <td><%= storage_place.unf_id %></td>
        <td><%= link_to 'Show', storage_place %></td>
        <td><%= link_to 'Edit', edit_storage_place_path(storage_place) %></td>
        <td><%= link_to 'Destroy', storage_place, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Storage Place', new_storage_place_path %>
